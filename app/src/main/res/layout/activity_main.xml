<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jeromesamuel.cst2335final.MainActivity"
    >



    <android.support.v7.widget.Toolbar

        android:id="@+id/toolbar_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?attr/actionBarSize"
        android:background="@color/colorPrimary"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="265dp"
        android:background="@drawable/algonquin"
         />

    <TextView
        android:textColor="@android:color/black"
        android:layout_margin="5dp"
        android:id="@+id/yahyaText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="32dp"
        android:fontFamily="sans-serif"
        android:text="@string/yahyaWelcome"/>
    <TextView
        android:textColor="@android:color/black"
        android:layout_margin="5dp"
        android:id="@+id/jeromeText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="32dp"
        android:fontFamily="sans-serif"
        android:text="@string/jeromeWelcome"/>




</LinearLayout>
